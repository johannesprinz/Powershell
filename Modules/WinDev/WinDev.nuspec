    <authors>Johannes Prinz</authors>
    <iconUrl>http://www.gravatar.com/avatar/a5eaccbfc33daeeb8001e0f39ca92849.png</iconUrl>
    <projectUrl>https://github.com/johannesprinz/Powershell</projectUrl>
    <licenseUrl>https://github.com/johannesprinz/Powershell/blob/master/LICENSE.md</licenseUrl>
  </metadata>
  <files>
    <file src="WinDev.psm1" target="tools" />
    <file src="Functions\**\*.*" target="tools\Functions" />
  </files>
</package>
